<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

    <meta name="viewport" content="width=device-width, initial-scale=1.0">


    <title>电子联盟后台管理系统</title>
   
    <link href="~/img/unionlogo.jpg" rel="shortcut icon" type="image/x-icon" />
    <link rel="shortcut icon" href="favicon.ico">
    <link href="https://cdn.bootcss.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdn.staticfile.org/font-awesome/4.4.0/css/font-awesome.css?v=4.4.0" rel="stylesheet">

    <link href="~/css/plugins/toast/animate.css" rel="stylesheet" />
    <link href="~/css/plugins/toast/toast.css" rel="stylesheet" />

    @*<link href="https://cdn.bootcss.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">*@
    <link href="~/css/style.min.css?v=4.1.0" rel="stylesheet">
    <link href="~/css/plugins/jqgrid/ui.jqgrid.css?0820" rel="stylesheet" />
    <style>
        /* Additional style to fix warning dialog position */

        #alertmod_table_list_2 {
            top: 900px !important;
        }
          .middle-style {
         
    position: absolute;
  width:100%;
  height:100%;
    background-color: white;
  
    z-index: 1000;
            
        }
       .middle-style .sk-spinner {
           
           position: absolute;
        
            left: 50%;
            top: 50%;
         
        }
    </style>

</head>

<body class="gray-bg">
    <div class="spiner-example middle-style" style="opacity: 0.8; z-index: 1000;">
        <div class="sk-spinner sk-spinner-wave">
            <div class="sk-rect1"></div>
            <div class="sk-rect2"></div>
            <div class="sk-rect3"></div>
            <div class="sk-rect4"></div>
            <div class="sk-rect5"></div>
        </div>
    </div>
    <div class="wrapper wrapper-content  animated toastdiv basicenterprise">
        <div class="row">
            <div class="col-sm-12">
                <div class="ibox ">
                    <div class="ibox-title">
                        <h5>企业会员信息</h5>
                        <div class="ibox-tools">
                            <a class="collapse-link">
                                <i class="fa fa-chevron-up"></i>
                            </a>

                        </div>
                    </div>
                    <div class="ibox-content">
                        <div class="row">
                            <div class="col-sm-12">
                                @*float-e-margins*@
                                <div class="ibox" style="margin-bottom:0px">
                                    <div class="ibox-title" style="border-width:1px 0 0">
                                        <h5>搜索条件</h5>

                                    </div>
                                    <div class="ibox-content">
                                        <form role="form" class="form-inline">
                                            <div class="form-group" style="margin-right:10px">
                                                <label for="membername" class="sr-only">会员名称</label>
                                                <input type="text" placeholder="请输入会员名称" id="membername" class="form-control">
                                            </div>
                                            <div class="form-group">
                                                <label for="memberregtime" class="">注册时间</label>
                                                <input type="date" id="memberregtime" class="form-control">
                                            </div>

                                            <button class="btn btn-primary search-enterprise-data" type="button">查询</button>
                                        </form>
                                    </div>
                                </div>
                            </div>

                        </div>

                        <hr>

                        @*table*@
                        <div class="jqGrid_wrapper">
                            <div class="ui-jqgrid" style="width: 1060px;height:400px;overflow-y:scroll">
                                <table id="table_list" class="table table-striped table-condensed table-bordered"></table>

                                <div class="ui-jqgrid-resize-mark" id="rs_mtable_list_2">&nbsp;</div>
                                <div id="pager_list_2" class="ui-jqgrid-pager" dir="ltr" style="width: 1060px;">
                                    <div id="pg_pager_list_2" class="ui-pager-control" role="group">
                                        <table class="ui-pg-table ui-common-table ui-pager-table">
                                            <tbody>
                                                <tr>
                                                    <td id="pager_list_2_left" align="left">
                                                        <table class="ui-pg-table navtable ui-common-table"></table>
                                                    </td>

                                                    <td id="pager_list_2_center" align="center" style=" width: 351px;">
                                                        <table class="ui-pg-table ui-common-table ui-paging-pager">
                                                            <tbody>
                                                                <tr>
                                                                    <td id="first_pager_list_2" class="ui-pg-button" title="First Page"><span class="glyphicon glyphicon-step-backward"></span></td>
                                                                    <td id="prev_pager_list_2" class="ui-pg-button" title="Previous Page"><span class="glyphicon glyphicon-backward"></span></td>
                                                                    <td class="ui-pg-button ui-disabled" style="cursor: default;"><span class="ui-separator"></span></td>
                                                                    <td id="input_pager_list_2" dir="ltr"> <input id="pagenowshow" class="ui-pg-input form-control" type="text" size="2" maxlength="7" value="0" role="textbox">  <span id="sp_1_pager_list_2">共0页</span> </td>
                                                                    <td class="ui-pg-button ui-disabled"><span class="ui-separator"></span></td>
                                                                    <td id="next_pager_list_2" class="ui-pg-button" title="Next Page"><span class="glyphicon glyphicon-forward"></span></td>
                                                                    <td id="last_pager_list_2" class="ui-pg-button" title="Last Page"><span class="glyphicon glyphicon-step-forward"></span></td>
                                                                    <td dir="ltr">
                                                                        <select class="ui-pg-selbox form-control" role="listbox" title="Records per Page">
                                                                            <option role="option" value="10" selected="selected">10</option>
                                                                            <option role="option" value="20">20</option>
                                                                            <option role="option" value="30">30</option>
                                                                        </select>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </td>

                                                    <td id="pager_list_2_right" align="right"><div dir="ltr" style="text-align:right" class="ui-paging-info totalnum">共0条</div></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                        @*table*@


                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="wrapper wrapper-content animated fadeInRight detailenterprise" style="display:none">

        <div class="row">
            <div class="col-sm-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5>
                            当前企业会员信息
                        </h5>
                        <div class="ibox-tools">
                            <a class="collapse-link">
                                <i class="fa fa-chevron-up"></i>
                            </a>

                        </div>
                    </div>
                    <div class="ibox-content">
                        <form method="get" class="form-horizontal">
                            <div class="form-group">
                                <label class="col-sm-2 control-label">头像</label>


                                <div class="col-sm-3">
                                    <div class="dropdown profile-element">
                                        <span>

                                            <img style="height:50px;width:50px" alt="image" class="img-circle" src="~/img/default-head.jpg" data-original="" id="detailinfo_head" name="detailinfo_head">
                                        </span>

                                    </div>

                                </div>
                                <label class="col-sm-2 control-label">会员编号</label>

                                <div class="col-sm-3">
                                    <input type="text" disabled class="form-control " id="detailinfo_num" name="detailinfo_num">
                                </div>
                            </div>
                            <div class="form-group">
                               
                                <label class="col-sm-2 control-label">注册手机号码</label>

                                <div class="col-sm-3">
                                    <input type="text" disabled class="form-control " id="detailinfo_phone" name="detailinfo_phone">
                                </div>
                                <label class="col-sm-2 control-label">姓名</label>

                                <div class="col-sm-3">
                                    <input type="text" disabled class="form-control " id="detailinfo_name" name="detailinfo_name">
                                </div>
                            </div>
                          
                            <div class="form-group">
                                <label class="col-sm-2 control-label">昵称</label>

                                <div class="col-sm-3">
                                    <input type="text" disabled class="form-control " id="detailinfo_nickname" name="detailinfo_nickname">
                                </div>
                                <label class="col-sm-2 control-label">企业名称</label>

                                <div class="col-sm-3">
                                    <input type="text" disabled class="form-control " id="detailinfo_entername" name="detailinfo_entername">
                                </div>
                            </div>
                          
                            <div class="form-group">
                                <label class="col-sm-2 control-label">企业所属行业</label>

                                <div class="col-sm-3">
                                    <input type="text" disabled class="form-control " id="detailinfo_enterofindustry" name="detailinfo_enterofindustry">
                                </div>
                                <label class="col-sm-2 control-label">企业介绍</label>

                                <div class="col-sm-3">
                                    <textarea type="text" disabled class="form-control " id="detailinfo_enterintroduce" name="detailinfo_enterintroduce"></textarea>
                                </div>
                            </div>
                         
                          
                         
                            <div class="form-group">
                                <label class="col-sm-2 control-label">企业加工能力及产品介绍</label>

                                <div class="col-sm-3">
                                    <textarea type="text" disabled class="form-control " id="detailinfo_producintroduce" name="detailinfo_producintroduce"></textarea>
                                </div>
                                <label class="col-sm-2 control-label">相关体系介绍</label>

                                <div class="col-sm-3">
                                    <textarea type="text" disabled class="form-control " id="detailinfo_relateintroduce" name="detailinfo_relateintroduce"></textarea>
                                </div>
                            </div>
                          
                                  
                         
                            <div class="form-group">
                                <label class="col-sm-2 control-label">企业电话</label>

                                <div class="col-sm-3">
                                    <input type="text" disabled class="form-control " id="detailinfo_enterphone" name="detailinfo_enterphone">
                                </div>
                                <label class="col-sm-2 control-label">企业传真</label>

                                <div class="col-sm-3">
                                    <input type="text" disabled class="form-control " id="detailinfo_fax" name="detailinfo_fax">
                                </div>
                            </div>
                            
                            <div class="form-group">
                                <label class="col-sm-2 control-label">邮箱</label>

                                <div class="col-sm-3">
                                    <input type="text" disabled class="form-control " id="detailinfo_email" name="detailinfo_emial">
                                </div>
                                <label class="col-sm-2 control-label">企业地址</label>

                                <div class="col-sm-3">
                                    <input type="text" disabled class="form-control " id="detailinfo_enteraddress" name="detailinfo_enteraddress">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-2 control-label">营业执照</label>

                                <div class="col-sm-3">
                                    <div class="dropdown profile-element">
                                        <span>
                                            <img style="height:200px;width:242px" alt="image" class="" src="~/img/id-card.png" data-original="" id="detailinfo_license" name="detailinfo_license">

                                        </span>


                                    </div>

                                </div>
                                <label class="col-sm-2 control-label">证书</label>

                                <div class="col-sm-3">
                                    <div class="dropdown profile-element">
                                        <span>
                                            <img style="height:200px;width:242px" alt="image" class="" src="~/img/id-card.png" data-original="" id="detailinfo_certificate" name="detailinfo_certificate">

                                        </span>


                                    </div>

                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-2 control-label">荣誉</label>

                                <div class="col-sm-3">
                                    <div class="dropdown profile-element">
                                        <span>
                                            <img style="height:200px;width:242px" alt="image" class="" src="~/img/id-card.png" data-original="" id="detailinfo_honor" name="detailinfo_honor">

                                        </span>


                                    </div>

                                </div>
                                <label class="col-sm-2 control-label">相关图片</label>

                                <div class="col-sm-3">
                                    <div class="dropdown profile-element">
                                        <span>
                                            <img style="height:200px;width:242px" alt="image" class="" src="~/img/id-card.png" data-original="" id="detailinfo_relatedimages" name="detailinfo_relatedimages">

                                        </span>


                                    </div>

                                </div>
                            </div>
                            <div class="form-group">

                             
                                <label class="col-sm-2 control-label">质量保证</label>

                                <div class="col-sm-3">
                                    <div class="dropdown profile-element">
                                        <span>
                                            <img style="height:200px;width:242px" alt="image" class="" src="~/img/id-card.png" data-original="" id="detailinfo_QA" name="detailinfo_QA">

                                        </span>


                                    </div>

                                </div>
                            </div>
                            <div class="form-group">

                                <label class="col-sm-2 control-label">注册时间</label>

                                <div class="col-sm-3">
                                    <input type="text" disabled class="form-control " id="detailinfo_regtime" name="detailinfo_regtime">
                                </div>
                              

                              
                            </div>

                            <div class="hr-line-dashed"></div>
                            <div class="form-group">
                                <div class="col-sm-4 col-sm-offset-2">
                                    @*<button class="btn btn-primary" type="submit">保存内容</button>*@
                                    <button class="btn btn-white backprepage" type="button">返回</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="https://cdn.staticfile.org/jquery/2.1.4/jquery.min.js"></script>
    <script src="https://cdn.bootcss.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
    <script src="~/Scripts/plugins/peity/jquery.peity.min.js"></script>
    <script src="@Url.Content("~/Scripts/plugins/lazyload/jquery.lazyload.min.js")"></script>
    <script src="~/Scripts/content.min.js?v=1.0.0"></script>
    <script src="~/Scripts/plugins/toast/toast.js"></script>
    <script type="text/javascript">var rootUrl = "@Url.Content("~")";//获取根路径，防止iis部署位置不同</script>
    <script src="@Url.Content("~/Scripts_App/Utilities/Utilities.js")"></script>
  
     <script src="@Url.Content("~/Scripts_App/MemberManage/MemberListEnterprise.js")"></script>
    <script>


    </script>

</body>
</html>