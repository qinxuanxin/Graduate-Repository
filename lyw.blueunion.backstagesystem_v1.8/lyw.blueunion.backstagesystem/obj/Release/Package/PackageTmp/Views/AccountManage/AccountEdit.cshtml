<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>电子联盟后台管理系统</title>
    <link href="@Url.Content("~/css/bootstrap.min.css?v=3.3.5")" rel="stylesheet" />
   
    <link href="@Url.Content("~/css/font-awesome.min.css?v=4.4.0")" rel="stylesheet" />
    
    <link href="@Url.Content("~/css/style.min.css?v=4.0.0")" rel="stylesheet" />
   
    <link href="@Url.Content("~/css/plugins/jasny/jasny-bootstrap.min.css")" rel="stylesheet" />
  
    <style>
          .middle-style {
         
    position: absolute;
  width:100%;
  height:100%;
    background-color: white;
  
    z-index: 1000;
            
        }
       .middle-style .sk-spinner {
           
           position: absolute;
        
            left: 50%;
            top: 50%;
         
        }
    </style>
    <link href="~/css/plugins/toast/animate.css" rel="stylesheet" />
    <link href="~/css/plugins/toast/toast.css" rel="stylesheet" />
    <script src="@Url.Content("~/Scripts/jquery-1.10.2.min.js")"></script>
    <script src="~/Scripts/plugins/toast/toast.js"></script>
    <script src="@Url.Content("~/Scripts_App/Utilities/Utilities.js")"></script>
    <script type="text/javascript">var rootUrl = "@Url.Content("~")";//获取根路径，防止iis部署位置不同</script>

    <script src="@Url.Content("~/Scripts_App/AccountManage/AccountManage.js")"></script>

</head>
<body class="gray-bg">
    <div class="spiner-example middle-style" style="opacity: 0.8; z-index: 1000;">
        <div class="sk-spinner sk-spinner-wave">
            <div class="sk-rect1"></div>
            <div class="sk-rect2"></div>
            <div class="sk-rect3"></div>
            <div class="sk-rect4"></div>
            <div class="sk-rect5"></div>
        </div>
    </div>
    <div class="wrapper wrapper-content animated toastdiv">

        <div class="row">
            <div class="col-sm-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5>账号记录查询</h5>
                        <div class="ibox-tools">

                            <a class="close-link">
                                <i class="fa fa-times"></i>
                            </a>
                        </div>
                    </div>
                    <div class="ibox-content">

                        <div class="row">
                            <div class="form-group">

                                <div class="col-lg-3 col-md-3">
                                    <input type="text" id="SearchAdminName" form="registerForm" class="form-control" placeholder="请输入用户名" />
                                </div>



                                @*<label class="control-label col-lg-2 col-md-2" style="padding-top:7px;width:10%">
                                        选择用户类型
                                    </label>*@

                                <div class="col-lg-3 col-md-3">
                                    <select id="SearchAdminId" name="Status" class="form-control">
                                        <option value="00">请选择用户类型</option>
                                        <option value="1">管理员</option>
                                        <option value="2">普通用户</option>

                                    </select>
                                </div>


                                <div class="col-md-4">
                                    <button type="button" class="form-control btn btn-primary search-accountinfo" data-loading-text="正在读取数据。。。">
                                        搜索
                                    </button>
                                </div>


                            </div>

                        </div>
                        <div class="table-responsive" style="padding-top:20px">
                            <table id="btnShowDeletetable" class="table table-bordered table-hover"></table>
                        </div>

                    </div>
                </div>
            </div>

        </div>
    </div>



</body>
</html>