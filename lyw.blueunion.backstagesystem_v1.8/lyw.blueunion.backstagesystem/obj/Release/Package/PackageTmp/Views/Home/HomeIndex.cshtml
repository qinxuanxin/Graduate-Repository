    $(function () {
        var username = "@ViewBag.username";
        adminid = "@ViewBag.adminid";
        var rank = "";
        document.getElementById("user-strong").innerHTML = "<strong class='font-bold'>" + username + "</strong>";
        if (adminid == "1")
            rank = "超级管理员";
        else if (adminid == "2")
            rank = "管理员";
