       <script>
 var uploader;
           $(function () {
     
           
               uploader = WebUploader.create({
                   auto: true,
                   swf: '@Url.Content("~/Scripts/plugins/webuploader/Uploader.swf")',
                   server: '@Url.Action("holdfile", "Home")',//控制器
                   pick: '#upl',
                   accept: {
